{
  "name": "inventry-management-system",
  "version": "1.0.0",
  "description": "inventry-management-system",
  "main": "app.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dist/app.js",
    "dev": "nodemon src/app.ts",
    "build": "npx tsc && tsc-alias",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:test": "export NODE_ENV=test && npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all && npx sequelize-cli db:drop && unset NODE_ENV",
    "ts.check": "tsc --project tsconfig.json",
    "add-build": "git add dist"
  },
  "keywords": [],
  "author": "Nishant Bhavsar",
  "dependencies": {
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "mysql2": "^3.14.0",
    "pg": "^8.14.1",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.13.15",
    "nodemon": "^3.1.9",
    "sequelize-cli": "^6.6.2",
    "tsc-alias": "^1.8.13",
    "typescript": "^5.8.2"
  },
  "_moduleAliases": {
    "@": "./src/"
  }
}
